@model IEnumerable<IndividualData>

<div class="container p-1">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary"> STOCK NAME AND FULL NAME</h2>
        </div>
    </div>
</div>
<br />

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Date and Time
            </th>
            <th>
                Open
            </th>
            <th>
                High
            </th>
            <th>
                Low
            </th>
            <th>
                Close
            </th>
            <th>
                Volume
            </th>
            <th>
                Weighted Close
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model)
        {
            <tr>
                <td>
                    @data.DateTime
                </td>
                <td>
                    @data.Open
                </td>
                <td>
                    @data.High
                </td>
                <td>
                    @data.Low
                </td>
                <td>
                    @data.Close
                </td>
                <td>
                    @data.Volume
                </td>
                <td>
                    @data.WClose
                </td>
            </tr>
        }
    </tbody>
</table>
